/* 引入字体图标 */
@import './styles/iconfont.wxss';

/* 样式初始化 */
page,view,text,swiper,swiper-item,navigator{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

/* 主题颜色 通过变量来实现
   1 less
   2 原生
*/
page{
    /*定义主题颜色*/
    --themeColor:#eb4450;
    /*定义同意字体大小  假如设计稿为375px 1px=2rpx */
    font-size: 28rpx;
}