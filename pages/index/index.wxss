view {
  /* 使用 主题颜色*/
  color: var(--themeColor);
}
.swiper-wrapper swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper-wrapper swiper image {
  width: 100%;
}
.cate-wrapper {
  display: flex;
}
.cate-wrapper navigator {
  padding: 20rpx;
  flex: 1;
}
.cate-wrapper navigator image {
  width: 100%;
}
.floor-wrapper {
  overflow: hidden;
}
.floor-wrapper .floor-guoup {
  padding: 10rpx 0;
}
.floor-wrapper .floor-guoup .floor-title image {
  width: 100%;
}
.floor-wrapper .floor-guoup .floor-list navigator {
  float: left;
  width: 33.33%;
}
.floor-wrapper .floor-guoup .floor-list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor-wrapper .floor-guoup .floor-list navigator:nth-last-child(-n+4):nth-child(2),
.floor-wrapper .floor-guoup .floor-list navigator:nth-last-child(-n+4):nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor-wrapper .floor-guoup .floor-list navigator image {
  height: 100%;
  width: 100%;
}
